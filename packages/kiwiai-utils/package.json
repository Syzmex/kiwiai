{
  "name": "kiwiai-utils",
  "version": "0.0.1",
  "description": "Kiwiai 辅助函数",
  "keywords": [
    "kiwiai",
    "webpack",
    "babel"
  ],
  "author": "Jokemmy <xu_rainy12@qq.com>",
  "homepage": "https://github.com/jokemmy/kiwiai#readme",
  "license": "ISC",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jokemmy/kiwiai.git"
  },
  "scripts": {
    "clean": "rimraf lib es",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "build": "npm run clean && npm run build:es && npm run build:commonjs && npm run build:flow && npm run build:flow:es",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --ignore '**/__tests__/**'",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib --ignore '**/__tests__/**'",
    "build:flow": "flow-copy-source -v -i '**/__tests__/**' src lib",
    "build:flow:es": "flow-copy-source -v -i '**/__tests__/**' src es",
    "prepare": "npm run clean && npm run lint && npm run build",
    "lint": "eslint --ext .js src",
    "precommit": "npm run lint"
  },
  "bugs": {
    "url": "https://github.com/jokemmy/kiwiai/issues"
  },
  "dependencies": {
    "@babel/register": "^7.0.0",
    "chalk": "^2.4.2",
    "debug": "^4.1.1",
    "json5": "^2.1.0",
    "strip-json-comments": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.2.3",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.1",
    "eslint-config-promise": "^2.0.1",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.14.0",
    "flow-bin": "^0.91.0",
    "flow-copy-source": "^2.0.2",
    "jest": "^23.6.0",
    "rimraf": "^2.6.3"
  }
}
