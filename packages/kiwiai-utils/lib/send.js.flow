//@flow

import { log } from './print';


// 信息状态
export const DONE = 'DONE';
export const STARTING = 'STARTING';
export const RESTART = 'RESTART';

export default function send( name: string, message: string ) {
  const processSend = process.send;
  if ( processSend ) {
    log( `Process[${name}]:send ${JSON.stringify( message )}` );
    processSend( message );
  }
}
